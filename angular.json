{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "@marrbo/json-model": {
      "projectType": "library",
      "root": "projects/marrbo/json-model",
      "sourceRoot": "projects/marrbo/json-model/src",
      "prefix": "lib",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:ng-packagr",
          "options": {
            "project": "projects/marrbo/json-model/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "projects/marrbo/json-model/tsconfig.lib.prod.json"
            },
            "development": {
              "tsConfig": "projects/marrbo/json-model/tsconfig.lib.json"
            }
          },
          "defaultConfiguration": "production"
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "tsConfig": "projects/marrbo/json-model/tsconfig.spec.json",
            "polyfills": [
              "zone.js",
              "zone.js/testing"
            ]
          }
        }
      }
    },
    "app-example": {
      "projectType": "application",
      "schematics": {},
      "root": "projects/app-example",
      "sourceRoot": "projects/app-example/src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:application",
          "options": {
            "outputPath": "dist/app-example",
            "index": "projects/app-example/src/index.html",
            "browser": "projects/app-example/src/main.ts",
            "polyfills": [
              "zone.js"
            ],
            "tsConfig": "projects/app-example/tsconfig.app.json",
            "assets": [
              "projects/app-example/src/favicon.ico",
              "projects/app-example/src/assets"
            ],
            "styles": [
              "projects/app-example/src/styles.css"
            ],
            "scripts": [],
            "server": "projects/app-example/src/main.server.ts",
            "prerender": true,
            "ssr": {
              "entry": "projects/app-example/server.ts"
            }
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "500kb",
                  "maximumError": "1mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "2kb",
                  "maximumError": "4kb"
                }
              ],
              "outputHashing": "all"
            },
            "development": {
              "optimization": false,
              "extractLicenses": false,
              "sourceMap": true,
              "fileReplacements": [
                {
                  "replace": "projects/app-example/src/environments/environment.ts",
                  "with": "projects/app-example/src/environments/environment.development.ts"
                }
              ]
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "configurations": {
            "production": {
              "buildTarget": "app-example:build:production"
            },
            "development": {
              "buildTarget": "app-example:build:development",
              "liveReload": true
            }
          },
          "defaultConfiguration": "development"
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "buildTarget": "app-example:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "polyfills": [
              "zone.js",
              "zone.js/testing"
            ],
            "tsConfig": "projects/app-example/tsconfig.spec.json",
            "assets": [
              "projects/app-example/src/favicon.ico",
              "projects/app-example/src/assets"
            ],
            "styles": [
              "projects/app-example/src/styles.css"
            ],
            "scripts": []
          }
        }
      }
    },
    "app-exemplo": {
      "projectType": "application",
      "schematics": {},
      "root": "projects/app-exemplo",
      "sourceRoot": "projects/app-exemplo/src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:application",
          "options": {
            "outputPath": "dist/app-exemplo",
            "index": "projects/app-exemplo/src/index.html",
            "browser": "projects/app-exemplo/src/main.ts",
            "polyfills": [
              "zone.js"
            ],
            "tsConfig": "projects/app-exemplo/tsconfig.app.json",
            "assets": [
              "projects/app-exemplo/src/favicon.ico",
              "projects/app-exemplo/src/assets"
            ],
            "styles": [
              "projects/app-exemplo/src/styles.css"
            ],
            "scripts": []
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "500kb",
                  "maximumError": "1mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "2kb",
                  "maximumError": "4kb"
                }
              ],
              "outputHashing": "all"
            },
            "development": {
              "optimization": false,
              "extractLicenses": false,
              "sourceMap": true,
              "fileReplacements": [
                {
                  "replace": "projects/app-exemplo/src/environments/environment.ts",
                  "with": "projects/app-exemplo/src/environments/environment.development.ts"
                }
              ]
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "configurations": {
            "production": {
              "buildTarget": "app-exemplo:build:production"
            },
            "development": {
              "buildTarget": "app-exemplo:build:development"
            }
          },
          "defaultConfiguration": "development"
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "buildTarget": "app-exemplo:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "polyfills": [
              "zone.js",
              "zone.js/testing"
            ],
            "tsConfig": "projects/app-exemplo/tsconfig.spec.json",
            "assets": [
              "projects/app-exemplo/src/favicon.ico",
              "projects/app-exemplo/src/assets"
            ],
            "styles": [
              "projects/app-exemplo/src/styles.css"
            ],
            "scripts": []
          }
        }
      }
    }
  },
  "cli": {
    "analytics": false
  }
}
